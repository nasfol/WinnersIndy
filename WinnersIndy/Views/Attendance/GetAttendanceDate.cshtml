@model IEnumerable<WinnersIndy.Data.Attendance>

@{
    ViewBag.Title = "GetAttendanceDate";
}

<h2>Get Attendance Date</h2>

<p>
    @Html.ActionLink("Create Attendance", "Create")
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.AttendanceDate)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.AttendanceDate)
            </td>
            <td>
                
                @Html.ActionLink("Details", "GetClassAttendance", new { id = item.AttendanceId })
               
            </td>
        </tr>
    }

</table>
